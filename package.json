{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build && rm -rf public/about-test && cp -r public/about public/about-test && rm public/component---src-pages-missing-resources*",
    "fullbuild": "gatsby-dev -sq && rm -rf public .cache && yarn build && yarn serve",
    "serve": "gatsby serve",
    "setup-cdn": "rm -rf public2/prefix && mkdir -p public2/prefix && cp -r public/* public2/prefix/"
  },
  "dependencies": {
    "gatsby": "^2.3.0",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-manifest": "^2.0.2-rc.0",
    "gatsby-plugin-netlify-cms": "^3.0.0-rc.0",
    "gatsby-plugin-offline": "^2.0.0-rc.0",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.0",
    "gatsby-plugin-sass": "^2.0.0-rc.0",
    "gatsby-plugin-twitter": "^2.0.0-rc.2",
    "gatsby-plugin-typography": "^2.2.0-rc.0",
    "gatsby-source-filesystem": "^2.0.1-rc.0",
    "gatsby-transformer-remark": "^2.3.8",
    "netlify-cms": "^2.0.9",
    "node-sass": "^4.9.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-theme-kirkham": "^0.16.3"
  }
}
